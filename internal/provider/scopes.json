[
    {
        "name": "Models",
        "description": "List models this organization has access to",
        "permissions_to_scopes": {
            "read": [
                "model.read",
                "api.model.read"
            ]
        },
        "endpoints": [
            "/v1/models"
        ]
    },
    {
        "name": "Model capabilities",
        "description": "Create chat completions, audio, embeddings, and images",
        "permissions_to_scopes": {
            "write": [
                "model.request",
                "api.model.request"
            ]
        },
        "endpoints": [
            "/v1/audio",
            "/v1/chat/completions",
            "/v1/embeddings",
            "/v1/images",
            "/v1/moderations"
        ]
    },
    {
        "name": "Assistants",
        "description": "Create and retrieve Assistants",
        "permissions_to_scopes": {
            "read": [
                "api.assistants.read"
            ],
            "write": [
                "api.assistants.write"
            ]
        },
        "endpoints": [
            "/v1/assistants",
            "/v1/models (required for Assistants)"
        ]
    },
    {
        "name": "Threads",
        "description": "Create and retrieve Threads/Messages/Runs",
        "permissions_to_scopes": {
            "read": [
                "api.threads.read"
            ],
            "write": [
                "api.threads.write"
            ]
        },
        "endpoints": [
            "/v1/threads",
            "/v1/models (required for Threads)"
        ]
    },
    {
        "name": "Fine-tuning",
        "description": "Create and retrieve fine tuning jobs",
        "permissions_to_scopes": {
            "read": [
                "api.fine_tuning.jobs.read"
            ],
            "write": [
                "api.fine_tuning.jobs.write"
            ]
        },
        "endpoints": [
            "/v1/fine_tuning"
        ]
    },
    {
        "name": "Files",
        "description": "Create and retrieve files",
        "permissions_to_scopes": {
            "read": [
                "api.files.read"
            ],
            "write": [
                "api.files.write"
            ]
        },
        "endpoints": [
            "/v1/files"
        ]
    }
]